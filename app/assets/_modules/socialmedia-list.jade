mixin socialmedia-list(bgClass)
	ul(class="socialmedia-list #{bgClass}")
		li
			a(href="https://www.linkedin.com/in/lagasa/" target="_blank")
				span.icon
				span.label Linkedin
		li
			a(href="https://www.instagram.com/lagasa/" target="_blank")
